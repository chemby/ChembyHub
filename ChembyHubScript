--Scripts

UserInputService.InputEnded:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.LeftAlt then
		if isOpened then
			Background.Visible = false
			isOpened = false
		else
			Background.Visible = true
			isOpened = true
		end	
	end
end)

Hubs.MouseButton1Click:Connect(function()
	About_2.Visible = false
	Settings_2.Visible = false
	MiscellanousLayer.Visible = false
	HubsLayer.Visible = true
	GameScriptsLayer.Visible = false
	About_2.Active = false
	Settings_2.Active = false
	MiscellanousLayer.Active = false
	HubsLayer.Active = true
	GameScriptsLayer.Active = false
end)

